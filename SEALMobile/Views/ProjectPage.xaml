<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="SEALMobile.Views.ProjectPage"
    NavigationPage.BackButtonTitle="Back">
    <ContentPage.Content>
        <StackLayout Margin="15">
            <Button  Text="EDGE device list"
                     Clicked="Handle_EdgeList"
                     HorizontalOptions="Center" 
                     HeightRequest="50" WidthRequest="180" Margin="20"
                     BackgroundColor="LightGray"
                     />
            <Button  Text="Keys Generator"
                     Clicked="Handle_KeyGenerator"
                     HorizontalOptions="Center" 
                     HeightRequest="50" WidthRequest="180" Margin="20"
                     BackgroundColor="LightGray"
                     />
            <Button  Text="Dashboards"
                     Clicked="Handle_Dashboards"
                     HorizontalOptions="Center" 
                     HeightRequest="50" WidthRequest="180" Margin="20"
                     BackgroundColor="LightGray"
                     />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
